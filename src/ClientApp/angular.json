{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ClientUI": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
            "options": {
                "poll": 1000,
                "outputPath": "dist",
                "index": "src/index.html",
                "main": "src/main.ts",
                "tsConfig": "src/../tsconfig.json",
                "showCircularDependencies": false,
                "assets": [
                    {
                        "glob": "signInCallback.html",
                        "input": "src",
                        "output": "/"
                    },
                    {
                        "glob": "silentCallback.html",
                        "input": "src",
                        "output": "/"
                    },
                    {
                        "glob": "**/*",
                        "input": "node_modules/@mdi/font/fonts",
                        "output": "/fonts"
                    },
                    {
                        "glob": "**/*",
                        "input": "node_modules/oidc-client/dist",
                        "output": "/oidc"
                    },
                    {
                        "glob": "**/*",
                        "input": "src/assets",
                        "output": "/"
                    }
                ],
                "styles": [
                    "node_modules/@mdi/font/css/materialdesignicons.css",
                    "node_modules/leaflet/dist/leaflet.css"
                ],
                "scripts": [
                    "node_modules/jquery/dist/jquery.min.js",
                    "node_modules/zone.js/dist/zone.js",
                    "node_modules/oidc-client/lib/oidc-client.js"
                ]
            },
            "configurations": {
                "production": {
                    "optimization": true,
                    "outputHashing": "all",
                    "sourceMap": false,
                    "extractCss": true,
                    "namedChunks": false,
                    "aot": true,
                    "extractLicenses": true,
                    "vendorChunk": false,
                    "buildOptimizer": true
                }
            }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ClientUI:build",
            "port": 5000,
            "host": "0.0.0.0",
            "hmr": true,
            "hmrWarning": false,
            "disableHostCheck": true
          },
          "configurations": {}
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ClientUI:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "./tsconfig.json"
            ],
            "exclude": []
          }
        }
      }
    },
    "ClientUI-e2e": {
      "root": "",
      "sourceRoot": "e2e",
      "projectType": "application"
    }
  },
  "defaultProject": "ClientUI",
  "cli": {
    "warnings": {
      "typescriptMismatch": false
    }
  },
  "schematics": {
    "@schematics/angular:component": {},
    "@schematics/angular:directive": {}
  }
}